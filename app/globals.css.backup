@theme {
  /* Colors */
  --color-primary-purple: #0b0518;
  --color-primary-purple-light: #0b0518;
  --color-primary-purple-dark: #0d0019;
  --color-accent-pink: #ff0055;
  --color-accent-pink-light: #ff3377;
  --color-accent-pink-dark: #cc0044;
  --color-neutral-white: #ffffff;
  --color-neutral-gray-light: #e5e5e5;
  --color-neutral-gray-dark: #333333;

  /* Spacing */
  --spacing-section: 5rem;
  --spacing-section-mobile: 3rem;

  /* Font sizes */
  --font-size-hero: 4rem;
  --font-size-hero-mobile: 2.5rem;
  --font-size-h1: 48.83px;
  --font-size-h2: 2rem;
  --font-size-h3: 1.5rem;

  /* Font families */
  --font-display: 'ATYP Display', system-ui, -apple-system, sans-serif;
  --font-body: 'ATYP Text', system-ui, -apple-system, sans-serif;
}

html {
  scroll-behavior: smooth;
}

body {
  background-color: var(--color-primary-purple);
  color: var(--color-neutral-white);
  font-family: var(--font-body);
}

.container-custom {
  max-width: 80rem;
  margin-left: auto;
  margin-right: auto;
  padding-left: 1rem;
  padding-right: 1rem;
}

@media (min-width: 640px) {
  .container-custom {
    padding-left: 1.5rem;
    padding-right: 1.5rem;
  }
}

@media (min-width: 1024px) {
  .container-custom {
    padding-left: 2rem;
    padding-right: 2rem;
  }
}

.section-padding {
  padding-top: var(--spacing-section-mobile);
  padding-bottom: var(--spacing-section-mobile);
}

@media (min-width: 768px) {
  .section-padding {
    padding-top: var(--spacing-section);
    padding-bottom: var(--spacing-section);
  }
}

.btn-primary {
  background-color: var(--color-accent-pink);
  color: white;
  font-weight: 600;
  padding: 0.75rem 2rem;
  border-radius: 9999px;
  transition: all 0.3s;
  display: inline-block;
}

.btn-primary:hover {
  background-color: var(--color-accent-pink-dark);
  transform: scale(1.05);
}

.btn-secondary {
  border: 1px solid white;
  color: white;
  font-weight: 600;
  padding: 0.75rem 2rem;
  border-radius: 9999px;
  transition: all 0.3s;
  display: inline-block;
}

.btn-secondary:hover {
  background-color: white;
  color: var(--color-primary-purple);
}

.heading-hero {
  font-family: var(--font-display);
  font-size: var(--font-size-hero-mobile);
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: -0.05em;
  line-height: 1.1;
}

@media (min-width: 768px) {
  .heading-hero {
    font-size: var(--font-size-hero);
  }
}

.heading-1 {
  font-family: var(--font-display);
  font-size: var(--font-size-h1);
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: -0.05em;
  line-height: 1.2;
}

.heading-2 {
  font-family: var(--font-display);
  font-size: var(--font-size-h2);
  font-weight: 600;
  text-transform: uppercase;
  line-height: 1.3;
}

.heading-3 {
  font-family: var(--font-display);
  font-size: var(--font-size-h3);
  font-weight: 600;
  text-transform: uppercase;
  line-height: 1.4;
}

.image-placeholder {
  background-color: rgb(88 28 135 / 0.5);
  border: 4px dashed rgb(192 132 252);
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  padding: 2rem;
  border-radius: 0.5rem;
}

.bg-gradient-custom {
  background: linear-gradient(135deg, #0b0518 0%, #0b0518 50%, #0b0518 100%);
}

.bg-gradient-overlay {
  background: radial-gradient(circle at top right, rgba(255, 0, 85, 0.1) 0%, transparent 50%);
}
